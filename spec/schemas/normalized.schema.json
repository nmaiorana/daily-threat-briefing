{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "NormalizedRecord",
  "type": "object",
  "required": ["id", "source", "published", "headline", "body"],
  "properties": {
    "id": { "type": "string" },
    "source": { "type": "string" },
    "published": { "type": "string", "format": "date-time" },
    "headline": { "type": "string" },
    "body": { "type": "string" }
  },
  "additionalProperties": false
}
